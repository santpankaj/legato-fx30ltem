version: $LEGATO_WIFI_VERSION

sandboxed: false

executables:
{
    wifiApTest = ( wifiApTestComponent )
}

processes:
{
    envVars:
    {
        LE_LOG_LEVEL = DEBUG
    }

    run:
    {
        (wifiApTest)
    }
}

start: manual

bindings:
{
    wifiApTest.wifiApTestComponent.le_wifiAp -> wifiService.le_wifiAp
}
